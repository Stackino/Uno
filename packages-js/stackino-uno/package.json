{
  "name": "@stackino/uno",
  "version": "0.1.21",
  "description": "Application framework formed from popular packages.",
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "init": "npm install",
    "dev": "n-clean ./dist ./es && webpack --mode development --output-pathinfo --watch",
    "prod": "n-clean ./dist ./es && tsc && n-copy --source ./src --destination ./es **/*.scss && webpack --mode production",
    "prepublishOnly": "npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check-updates": "ncu --reject mobx",
    "upgrade": "ncu -u -a --reject mobx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stackino/Uno.git"
  },
  "author": "Lukáš Novotný",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Stackino/Uno/issues"
  },
  "homepage": "https://github.com/Stackino/Uno#readme",
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.7.1",
    "@types/react-dom": "^16.0.9",
    "@types/webpack-env": "^1.13.6",
    "css-loader": "^1.0.1",
    "node-sass": "^4.10.0",
    "npm-build-tools": "^2.2.5",
    "reflect-metadata": "^0.1.12",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@uirouter/react": "^0.8.7",
    "classnames": "^2.2.6",
    "core-decorators": "^0.20.0",
    "inversify": "^5.0.1",
    "inversify-react": "^0.3.0",
    "mobx": "^4.3.1",
    "mobx-react": "^5.3.6",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "topbar": "^0.1.3",
    "tslib": "^1.9.3"
  }
}
